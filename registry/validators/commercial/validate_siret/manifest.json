{
  "id": "azalscore.validators.commercial.validate_siret",
  "name": "Validation SIRET",
  "category": "validation",
  "version": "1.0.0",
  "description": "Valide un numéro SIRET français (14 chiffres avec algorithme de Luhn)",
  "inputs": {
    "siret": {
      "type": "string",
      "required": true,
      "description": "Numéro SIRET à valider (14 chiffres)"
    }
  },
  "outputs": {
    "is_valid": {
      "type": "boolean",
      "description": "true si le SIRET est valide"
    },
    "normalized_siret": {
      "type": "string",
      "description": "SIRET normalisé (sans espaces)"
    },
    "siren": {
      "type": "string",
      "description": "SIREN extrait (9 premiers chiffres)"
    },
    "nic": {
      "type": "string",
      "description": "NIC extrait (5 derniers chiffres)"
    },
    "error": {
      "type": "string",
      "description": "Message d'erreur si invalide (null si valide)"
    }
  },
  "side_effects": false,
  "idempotent": true,
  "no_code_compatible": true,
  "retry_strategy": {
    "max_attempts": 1,
    "timeout_ms": 1000,
    "fallback": null
  },
  "dependencies": [],
  "tags": ["validation", "siret", "siren", "france", "business"],
  "author": "AZALSCORE",
  "license": "Proprietary",
  "created_at": "2026-01-22",
  "updated_at": "2026-01-22"
}
